<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="org.wfq.odo.dao.secondary.StudentMapper" >
  <resultMap id="BaseResultMap" type="org.wfq.odo.entity.secondary.Student" >
    <id column="id" property="id" jdbcType="INTEGER" />
    <result column="stu_name" property="stuName" jdbcType="VARCHAR" />
    <result column="stu_id" property="stuId" jdbcType="VARCHAR" />
  </resultMap>
  <sql id="Base_Column_List" >
    id, stu_name, stu_id
  </sql>
  <select id="getStuNameById" parameterType="java.lang.Integer" resultMap="BaseResultMap">
    select stu_name from student where id = #{id}
  </select>
</mapper>